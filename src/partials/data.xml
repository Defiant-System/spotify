		<i name="search" match="//Search" template="search-view"/>
		<i name="search-tracks" url="/search?q={query}&amp;type=track&amp;market={market}" match="//SearchTracks" template="playlist"/>
		<i name="search-artists" url="/search?q={query}&amp;type=artist&amp;market={market}" match="//SearchArtists" template="artists"/>
		<i name="search-albums" url="/search?q={query}&amp;type=album&amp;market={market}" match="//SearchAlbums" template="mixed-albums"/>
