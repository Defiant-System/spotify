<Data>
	<Records>
		<i name="loading" match="//Home" template="loading-full-view"/>
		<i name="get-my-playlists" url="/users/{username}/playlists?offset=0&amp;limit=50" match="//Playlists" template="my-playlists"/>

		<i name="home" match="//Home" template="home-view"/>
		<i name="home-browse" url="/browse/categories" match="//Categories" template="home-browse"/>
		<i name="home-featured" url="/browse/featured-playlists" match="//Featured" template="home-featured"/>
		<i name="home-favorites" url="/me/top/tracks" match="//Favorites" template="playlist"/>
		<i name="home-history" url="/me/player/recently-played" match="//Recently" template="playlist"/>

		<i name="search" match="//Search" template="search-view"/>
		<i name="search-tracks" url="/search?q={query}&amp;type=track&amp;market={market}" match="//SearchTracks" template="playlist"/>
		<i name="search-artists" url="/search?q={query}&amp;type=artist&amp;market={market}" match="//SearchArtists" template="artists"/>
		<i name="search-albums" url="/search?q={query}&amp;type=album&amp;market={market}" match="//SearchAlbums" template="mixed-albums"/>
		<i name="search-playlists" url="/search?q={query}&amp;type=playlist&amp;market={market}" match="//SearchPlaylists" template="playlists"/>

		<i name="show-category" url="/browse/categories/{categoryId}" match="//Category" template="category-view"/>
		<i name="show-category-playlists" url="/browse/categories/{categoryId}/playlists" match="//CategoryPlayLists" template="categories-view"/>
		<i name="show-compilation" url="/playlists/{categoryId}" match="//CategoryPlayList" template="compilation-view"/>
		<i name="show-playlist" url="/playlists/{id}?market={market}" match="//Playlist" template="playlist-view" fields="name,type,uri,owner(display_name),images,tracks(items(track(name,uri,duration_ms,artists(name,uri),album(name,uri))))"/>
		<i name="show-album" url="/albums/{id}?market={market}" match="//Album" template="album-view"/>
		<i name="show-featured" url="/playlists/{id}?market={market}" match="//Playlist" template="compilation-view"/>

		<i name="show-artist" url="/artists/{artistId}" match="//Artist" template="artist-view"/>
		<i name="show-artist-top-tracks" url="/artists/{artistId}/top-tracks?market={market}" match="//ArtistTopTracks" template="artist-top-tracks"/>
		<i name="show-artist-albums" url="/artists/{artistId}/albums?market={market}" match="//ArtistAlbums" template="artist-albums"/>
		<i name="show-artist-appears-on" url="/artists/{artistId}/albums?market={market}" match="//ArtistAppearsOn" template="mixed-albums"/>
		<i name="show-artist-related" url="/artists/{artistId}/related-artists" match="//ArtistRelated" template="artists"/>
		<i name="show-artist-albums-album" url="/albums/{albumId}/tracks?market={market}" match="//ArtistAlbumsAlbum" template="artist-album"/>
	</Records>

	<Login />
	<Home />
	<Featured />
	<Playlists />
	<Favorites />
	<Categories />
	<Category />
	<CategoryPlayLists />
	<CategoryPlayList />
	<Recently />
	<Artist />
	<ArtistTopTracks />
	<ArtistAlbums />
	<ArtistAppearsOn />
	<ArtistRelated />
	<ArtistAlbumsAlbum />
	<Playlist />
	<Album />
	
	<Search />
	<SearchTracks />
	<SearchArtists />
	<SearchAlbums />
	<SearchPlaylists />

</Data>