		<i name="search-genre" url="/search?q=genre:{id}&amp;type=artist&amp;market={market}" match="//SearchGenreArtists" template="genre-view"/>

		<i name="show-category" url="/browse/categories/{categoryId}" match="//Category" template="category-view"/>
